{
    "name": "symfony-product-crud",
    "description": "A Symfony project for managing products with a CRUD interface.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/orm-pack": "^2.0",
        "symfony/twig-pack": "^2.0",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/webpack-encore-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "Symfony\\Bundle\\MakerBundle\\Composer\\ScriptHandler::buildBootstrap": "build"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}