{% extends 'front_office_dashboard/base.html.twig' %}
{% block title %}About{% endblock %}

{% block banner %}
    <!--================= Banner Section Start Here =================-->
    <div class="banner banner1">
        <div class="inner-page-banner banner-bg">
            <div class="container">
                <div class="banner-content">
                    <div class="page-path">
                        <ul>
                            <li><a class="home-page-link" href="{{ path('app_home1') }}">Home</a></li>
                            <li><a class="current-page" href="#">About Us</a></li>
                        </ul>
                    </div>
                    <h1 class="banner-heading">ABOUT US</h1>
                </div>
            </div>
        </div>
    </div>
    <!--================= Banner Section End Here =================-->
{% endblock %}

{% block content %}
    <!--================= About Section Start Here =================-->
    <div class="rts-about-us-section section-gap">
        <div class="container">
            <div class="about-inner">
                <div class="about-picture"><img src="{{ asset('assets/images/about/section1.png') }}" alt=""></div>
                <div class="contents">
                    <div class="section-title-area section-title-area1 text-start">
                        <h1 class="section-title">ABOUT OUR SPORTIUS</h1>
                        <p>It is one of the most popular football clubs that offer training programs
                            for the youth, as well as summer camps. Play the fastest NFT based fantasy
                            football manager earn coins, collect & trade officially.</p>
                    </div>

                    <div class="about-plan">
                        <div class="plan-item">
                            <h4>OUR MISSION</h4>
                            <p>There are around 60 national teams,
                                60 clubs & total of 2000 player.</p>
                        </div>
                        <div class="plan-item">
                            <h4>OUR VISION</h4>
                            <p>Club is a free to play football game where
                                you can build.</p>
                        </div>
                    </div>
                    <a href="#rts-champions-section" class="more-btn">LEARN MORE</a>
                </div>
            </div>
        </div>
    </div>
    <!--================= About Section End Here =================-->

    <!--================= Champions Section Start Here =================-->
    <div class="rts-champion-section section-gap section-bg">
        <div class="container">
            <div class="top-wrap">
                <div class="filter-button-group">
                    <button class="filter-btn active" data-show=".yr90">1990</button>
                    <button class="filter-btn" data-show=".yr10">2010</button>
                    <button class="filter-btn" data-show=".yr12">2012</button>
                    <button class="filter-btn" data-show=".yr15">2015</button>
                    <button class="filter-btn" data-show=".yr22">2024</button>
                </div>
            </div>
            <div class="champions-inner">
            </div>
            <div class="champion-details filterd-items yr90">
                <div class="contents">
                    <h1 class="details-title">THE CHAMPIONS LEAGUE TROPHY 2010</h1>
                    <p class="p1">It is one of the most popular football clubs that offer training programs
                        for the youth, as well as summer camps. Play the fastest NFT based fantasy
                        football manager earn coins, collect & trade officially.</p>
                    <p class="p2">Welcome to the official Chelsea FC website. Get all the latest news, videos
                        & ticket information as well as player profiles.</p>
                </div>
                <div class="details-video">
                    <div class="video-thumb">
                        <img src="{{ asset('assets/images/about/video1.png') }}" alt="video-thumb">
                        <div class="play-btn play-video">
                            <a class="popup-video" href="https://www.youtube.com/watch?v=G4t6TqG5LM8" data-effect="mfp-move-horizontal">
                                <i class="fas fa-play"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Repeat champion-details for other years -->
        </div>
    </div>
    <!--================= Champions Section End Here =================-->

    <!--================= Blog Section Start Here =================-->
    <div class="rts-blog-section section-gap">
        <div class="container">
            <div class="section-title-area section-title-area-inner text-center mb--30">
                <h1 class="section-title">TEAM INSIGHTS</h1>
                <p>It is one of the most popular football clubs that offer training programs
                    for the youth, as well as summer camps</p>
            </div>
            <div class="blog-area">
                <div class="row justify-content-center">
                    <div class="col-xl-4 col-md-6">
                        <div class="blog-item">
                            <div class="blog-picture"><img src="{{ asset('assets/images/blog/blog1.jpg') }}" alt=""></div>
                            <div class="contents-wrapper">
                                <div class="contents">
                                    <div class="d-block">
                                        <a href="#" class="blog-catagory">FOOTBALL</a>
                                        <a href="#" class="blog-title">THE WORLD CUP â€“ WHAT TO EXPECT FROM ENGLAND FOOTBALL CLUB</a>
                                    </div>
                                    <div class="author-info">
                                        <div class="author-dp"><img src="{{ asset('assets/images/items/author2.png') }}" alt="author-dp"></div>
                                        <div class="content">
                                            <a href="#" class="author-name">CLUB DA AUTHOR</a>
                                            <div class="blog-date">
                                                <div class="date">June 21, 2024</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Repeat blog items for other posts -->
                </div>
            </div>
        </div>
    </div>
    <!--================= Blog Section End Here =================-->

    <!--================= Sponsors Section Start Here =================-->
    <div class="rts-sponsors-section section-gap section-bg">
        <div class="container">
            <div class="section-title-area section-title-area2 text-center mb--50">
                <h1 class="section-title">HAPPY SPONSORS</h1>
                <p>It is one of the most popular football clubs that offer training programs
                    for the youth, as well as summer camps</p>
            </div>
            <div class="row">
                <div class="col-xl-2 col-md-4 col-sm-6">
                    <a href="#" class="sponsor-single"><img src="{{ asset('assets/images/brands/1.png') }}" alt="sponsor"></a>
                </div>
                <!-- Repeat sponsor items -->
            </div>
        </div>
    </div>
    <!--================= Sponsors Section End Here =================-->
{% endblock %}

{% block footer %}
    <!--================= Newsletter Popup Start Here =================-->
    <div class="rts-newsletter-popup">
        <div class="newsletter-close-btn"><i class="fal fa-times"></i></div>
        <div class="newsletter-inner">
            <h3 class="newsletter-heading">Get Weekly Newsletter</h3>
            <p>Priyoshop has brought to you the Hijab 3 Pieces Combo Pack PS23. It is a completely modern design</p>
            <form>
                <div class="input-area">
                    <div class="input-div"><input type="text" placeholder="Your name">
                        <div class="input-icon"><i class="far fa-user"></i></div>
                    </div>
                    <div class="input-div"><input type="email" placeholder="Email address" required>
                        <div class="input-icon"><i class="far fa-envelope"></i></div>
                    </div>
                </div>
                <button type="submit" class="subscribe-btn">Subscribe Now <i class="fal fa-long-arrow-right ml--5"></i></button>
            </form>
        </div>
    </div>
    <!--================= Newsletter Popup End Here =================-->
{% endblock %}